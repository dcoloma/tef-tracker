{
  "manifest_version": 2, 
  "name": "FirefoxOS Tracker",
  "version": "3",
  "content_security_policy": "script-src 'self' https://*.firebaseio.com; object-src 'self'",
  "minimum_chrome_version": "31",
   "app": {
      "launch": {
         "local_path": "home.html"
      }
   },
   "icons": { "16": "style/icons/firefox-16.png",
           "48": "style/icons/firefox-48.png",
          "128": "style/icons/firefox-128.png" }
}